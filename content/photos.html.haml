#photos
//thumbs_for(country.parameterize.underscore).each do |image|
%script{src:"//fb.me/react-with-addons-0.12.2.js"}
%script{type:"text/javascript", src:'/scripts/photos.js'}
:javascript
  function start() {
    var countries = #{thumbnail_json};
    var photo = React.createFactory(Photos);
    React.render(photo({countries: countries}), document.getElementById('photos'));
  };
  if (window.addEventListener)
  {
    window.addEventListener('load', start, false);
  }
  else if (window.attachEvent)
  {
    window.attachEvent('onload', start);
  }
